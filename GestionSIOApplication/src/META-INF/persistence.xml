<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="GestionSIOApplicationPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>Classes.Utilisateur</class>
    <class>Classes.Professionnel</class>
    <class>Classes.Professeur</class>
    <class>Classes.Visite</class>
    <class>Classes.Etudiant</class>
    <class>Classes.Organisation</class>
    <class>Classes.Stage</class>
    <class>Classes.Option</class>
    <class>Classes.Personne</class>
    <class>Classes.Annee</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/gestion_sio?zeroDateTimeBehavior=convertToNull&amp;serverTimezone=Europe/Paris"/>
      <property name="javax.persistence.jdbc.user" value="gestion_sio_user"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="secret"/>
    </properties>
  </persistence-unit>
</persistence>
